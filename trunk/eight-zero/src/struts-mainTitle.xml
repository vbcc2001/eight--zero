<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN" "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="theme" extends="struts-default">
		<action name="mianTitle_*"  class="mainTitleAction" method="{1}">
			<result name="success">/jsp/theme.jsp</result>
		</action>
		<action name="gettype_*" class="mainTitleAction" method="getType">
			<result name="gettype_success">/jsp/gettype.jsp</result>
		</action>
	</package>
</struts>    
    
